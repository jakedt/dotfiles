[user]
	name = Jimmy Zelinskie
	email = jimmyzelinskie@gmail.com
[color]
	ui = auto
[alias]
	s = status
	h = show HEAD
	d = diff
	l = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr)%C(bold blue) -%an%Creset' --abbrev-commit
	co = checkout
	br = branch
	unstage = reset HEAD
	discard = checkout HEAD --
	ds = diff --cached
	pr = pull --rebase
[core]
	excludesfile = ~/.gitignore
[diff]
	mnemonicprefix = true
	algorithm = patience
[merge]
	tool = vimdiff
[difftool]
	prompt = false
[url "git@github.com:"]
	pushInsteadOf = git://github.com/
[push]
	default = simple
[github]
	user = jzelinskie
